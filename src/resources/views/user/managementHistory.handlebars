<div class="home">
    <div class="header-home">
        <div class="text">History</div>

        <div class="header-home-content">
            <div class="home-search">
                <input id="searchtext" type="search" placeholder="What are you looking for?">
                <button id="search_btn"><i class='bx bx-search icon'></i></button>
            </div>

            <button id="cart">
                <i class='bx bx-shopping-bag'></i>
                <small class="items_amount">0</small>
            </button>

            <ul id="noti_nav">
                <li>
                    <div class="btn_info" style="line-height: 55px;margin-left: 6px;"><i class="bx bx-bell icon"></i>
                    </div>
                    <div class="subnav">
                        {{#each user.notification}}
                        <p>{{time}}</p>
                        <p>{{content}}</p>

                        {{/each}}
                    </div>
                </li>
            </ul>
            <div class="account">
                <div class="account-img">
                    <img src="data:image/png;base64,"
                        alt="">
                </div>
                <div class="account-text">
                    <p class="account-name">{{user.surName}} {{user.name}}</p>
                    <p style="font-size: 14px;float: left;opacity: 0.6;margin-right: 5px;">Số dư :</p>
                    <p class="account-permission">{{user.AccountBalance}}</p>
                </div>
            </div>

            <div id="myModal" class="modal">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title">Giỏ Hàng</h5>
                        <span class="close_modal">&times;</span>
                    </div>
                    <div class="modal-body">
                        <div class="cart-row">
                            <span class="cart-stt cart-header cart-column">Stt</span>
                            <span class="cart-item cart-header cart-column">Sản Phẩm</span>
                            <span class="cart-price cart-header cart-column">Giá</span>
                            <span class="cart-quantity cart-header cart-column">Số Lượng</span>
                        </div>
                        <div class="cart-items">

                        </div>
                        <div class="cart-total">
                            <strong class="cart-total-title">Tổng Cộng:</strong>
                            <span class="cart-total-price">0 VNĐ</span>
                        </div>

                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary close-footer">Đóng</button>
                            <button type="button" class="btn btn-primary order">Thanh Toán</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="body_history">
        <div class="tm-section-wrap">
            <div class="home-header-text">
                <p>Lịch sử quản lý</p>
            </div>

            <div id="container-home_history">
                <div class="id1 exchangeH_table">
                    <div class="content_history">
                        <table class="exchange_history">
                            <h2>LỊCH SỬ CHUYỂN ĐỔI</h2><br>
                            <tr class="thead__tr-product">
                                <th class="stt_exchangeHistory stt_exchangeHistory1">STT</th>
                                <th class="time_exchangeHistory1">Thời gian thực hiện</th>
                                <th class="status_exchangeHistory">Trạng thái cũ</th>
                                <th class="status_exchangeHistory">Trạng thái mới</th>
                                <th class="placeTreat_exchangeHistory">Nơi điều trị cũ</th>
                                <th class="placeTreat_exchangeHistory">Nơi điều trị mới</th>

                            </tr>
                            <tbody>
                                {{#each user.exchangeHistory}}
                                <tr class="tr__{{this.id}} tbody__tr-product">
                                    <td class="stt_exchangeHistory stt_exchangeHistory1">{{@index}}</td>
                                    <td class="time_exchangeHistory1">{{time}}</td>
                                    <td class="status_exchangeHistory">{{statusOld}} </td>
                                    <td class="status_exchangeHistory">{{status}} </td>
                                    <td class="placeTreat_exchangeHistory">{{activeOld}}</td>
                                    <td class="placeTreat_exchangeHistory">{{active}}</td>
                                </tr>
                                {{/each}}
                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <div class="body_history">
        <div class="tm-section-wrap">
            <div class="home-header-text">
                <p>Lịch sử thanh toán</p>
            </div>

            <div id="container-home_history">
                <div class="id1">
                    <div class="content_history">
                        <table class="exchange_history">
                            <h2>LỊCH SỬ CHUYỂN ĐỔI</h2><br>
                            <tr class="thead__tr-product">
                                <th class="stt_payHistory">STT</th>
                                <th class="time_payHistory">Thời gian thực hiện</th>
                                <th>Số tiền cũ</th>
                                <th>Số tiền mới</th>
                                <th class="active_payHistory">Hoạt động</th>
                            </tr>
                            <tbody>
                                {{#each user.payHistory}}
                                <tr class="payHistory">
                                    <td class="stt_payHistory">{{@index}}</td>
                                    <td class="time_payHistory">{{time}}</td>
                                    <td class="td__name-{{this.id}}">{{statusOld}} </td>
                                    <td class="curr_payHistory">{{currentMoney}}</td>
                                    <td class="active_payHistory">{{active}}</td>
                                </tr>
                                {{/each}}
                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>